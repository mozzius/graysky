{
  "extends": "@graysky/tsconfig/base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./src/*"
      ]
    },
    "jsx": "react-native",
    "tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json",
    "types": [
      "nativewind/types"
    ]
  },
  "include": [
    "expo-plugins",
    "*.ts",
    "*.js",
    "index.tsx",
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
