{
  "extends": "@graysky/tsconfig/base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"]
    },
    "tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json",
    "types": ["@atcute/bluesky/lexicons"]
  },
  "include": ["*.ts", "*.js", "src"],
  "exclude": ["node_modules"]
}
